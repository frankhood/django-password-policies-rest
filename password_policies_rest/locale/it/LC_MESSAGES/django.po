#: password_policies_rest/templatetags/password_policies_rest_tags.py:20
#, python-format
msgid "must be at least 1 character long"
msgid_plural "must be at least %(count)s characters long"
msgstr[0] "deve contenere almeno un carattere"
msgstr[1] "deve contenere almeno %(count)s caratteri"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:27
msgid "must not be a common sequence of characters"
msgstr "non deve essere una sequenza di caratteri comune"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:32
#, python-format
msgid "must not contain %(count)s or more consecutive identical characters"
msgstr "non deve contenere %(count)s o più caratteri identici consecutivi"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:37
msgid "must be strong enough."
msgstr "deve essere abbastanza forte."

#: password_policies_rest/templatetags/password_policies_rest_tags.py:41
msgid "must not be based on a dictionary word."
msgstr "non deve essere basata su una parola del dizionario."

#: password_policies_rest/templatetags/password_policies_rest_tags.py:45
#, python-format
msgid "must contain at least 1 alphanumeric character"
msgid_plural "must contain at least %(count)s alphanumeric characters"
msgstr[0] "deve contenere almeno 1 carattere alfanumerico"
msgstr[1] "deve contenere almeno %(count)s caratteri alfanumerici"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:52
#, python-format
msgid "must contain at least 1 lowercase character"
msgid_plural "must contain at least %(count)s lowercase characters"
msgstr[0] "deve contenere almeno 1 carattere minuscolo"
msgstr[1] "deve contenere almeno %(count)s caratteri minuscoli"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:60
#, python-format
msgid "must contain at least 1 uppercase character"
msgid_plural "must contain at least %(count)s uppercase characters"
msgstr[0] "deve contenere almeno 1 carattere maiuscolo"
msgstr[1] "deve contenere almeno %(count)s caratteri maiuscoli"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:68
#, python-format
msgid "must contain at least 1 number"
msgid_plural "must contain at least %(count)s numbers"
msgstr[0] "deve contenere almeno 1 numero"
msgstr[1] "deve contenere almeno %(count)s numeri"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:75
#, python-format
msgid "must contain at least 1 special character (e.g. @#$%%^&.)"
msgid_plural ""
"must contain at least %(count)s special characters (e.g. @#$%%^&.)"
msgstr[0] "deve contenere almeno 1 carattere speciale (es. @#$%%^&.)"
msgstr[1] "deve contenere almeno %(count)s carattere speciale (es. @#$%%^&.)"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:81
msgid "must not be an email."
msgstr "non deve essere una mail."

#: password_policies_rest/templatetags/password_policies_rest_tags.py:88
#: password_policies_rest/templatetags/password_policies_rest_tags.py:95
#, python-format
msgid "Make sure it differs from the last password"
msgid_plural "Make sure it differs from the last %(count)s passwords"
msgstr[0] "Assicurati che differisca dall’ ultima password"
msgstr[1] "Assicurati che differisca dalle ultime %(count)s password"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:101
msgid "Make sure it is not similar to the old password"
msgstr "Assicurati che non sia simile alla vecchia password"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:108
msgid "Password ok!"
msgstr "Password ok!"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:109
msgid "Old password is required"
msgstr "La vecchia password è obbligatoria"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:110
msgid "Passwords do not match"
msgstr "Le password non coincidono"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:111
msgid "Password match!"
msgstr "Le password coincidono!"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:112
msgid "must be strong enough"
msgstr "deve essere abbastanza forte"

#: password_policies_rest/templatetags/password_policies_rest_tags.py:116
msgid "The new password must have the following characteristics:"
msgstr "La nuova password deve avere le seguenti caratteristiche:"
